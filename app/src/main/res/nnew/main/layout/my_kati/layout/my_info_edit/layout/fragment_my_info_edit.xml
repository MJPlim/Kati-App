<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/kati_light_gray"
    tools:context="com.plim.kati_app.kati.domain.nnew.main.myKati.myInfoEdit.MyInfoEditFragment">

    <com.plim.kati_app.kati.crossDomain.domain.view.etc.JSHInfoItem
        android:id="@+id/myInfoEditFragment_changeBirthItem"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        app:JSHInfoItem_content="정상"
        app:JSHInfoItem_title="생일"
        app:layout_constraintTop_toBottomOf="@+id/myInfoEditFragment_changeNameItem" />

    <com.plim.kati_app.kati.crossDomain.domain.view.etc.JSHInfoItem
        android:id="@+id/myInfoEditFragment_changeAddressItem"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        app:JSHInfoItem_content="정상"
        app:JSHInfoItem_title="주소"
        app:layout_constraintTop_toBottomOf="@+id/myInfoEditFragment_changeBirthItem" />

    <com.plim.kati_app.kati.crossDomain.domain.view.etc.JSHInfoItem
        android:id="@+id/myInfoEditFragment_changeNameItem"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:JSHInfoItem_content="정상"
        app:JSHInfoItem_title="닉네임"
        app:layout_constraintTop_toBottomOf="@+id/myInfoEditFragment_toolbar" />

    <com.plim.kati_app.kati.crossDomain.domain.view.etc.JSHToolBar
        android:id="@+id/myInfoEditFragment_toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:button_image="@drawable/jsh_before"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@color/white"
        android:elevation="5dp"
        app:title="내 정보 수정" />

    <com.plim.kati_app.kati.crossDomain.domain.view.etc.JSHInfoItem
        android:id="@+id/myInfoEditFragment_restoreEmailItem"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        app:JSHInfoItem_content="sh199919@gmail.com"
        app:JSHInfoItem_title="복구 이메일"
        app:layout_constraintTop_toBottomOf="@+id/myInfoEditFragment_changeAddressItem" />

    <com.plim.kati_app.kati.crossDomain.domain.view.etc.JSHInfoItem
        android:id="@+id/myInfoEditFragment_editPasswordItem"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        app:JSHInfoItem_content="비밀번호 변경"
        app:JSHInfoItem_title="비밀번호"
        app:layout_constraintTop_toBottomOf="@+id/myInfoEditFragment_restoreEmailItem" />


    <Button
        android:id="@+id/myInfoEditFragment_editPasswordButton"
        android:layout_width="wrap_content"
        android:minWidth="0dp"
        android:layout_height="40dp"
        android:layout_marginRight="16dp"
        android:backgroundTint="@color/white"
        android:text="변경"
        android:textColor="@color/black"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/myInfoEditFragment_editPasswordItem"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/myInfoEditFragment_restoreEmailItem" />


    <Button
        android:id="@+id/myInfoEditFragment_restoreEmailButton"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginRight="16dp"
        android:backgroundTint="@color/white"
        android:minWidth="0dp"
        android:text="변경"
        android:textColor="@color/black"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/myInfoEditFragment_restoreEmailItem"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/myInfoEditFragment_changeAddressItem" />


    <Button
        android:id="@+id/myInfoEditFragment_editNameButton"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginRight="16dp"
        android:backgroundTint="@color/white"
        android:minWidth="0dp"
        android:text="변경"
        android:textColor="@color/black"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/myInfoEditFragment_changeNameItem"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/myInfoEditFragment_changeNameItem" />

    <Button
        android:id="@+id/myInfoEditFragment_changeAddressButton"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginRight="16dp"
        android:backgroundTint="@color/white"
        android:minWidth="0dp"
        android:text="변경"
        android:textColor="@color/black"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/myInfoEditFragment_changeAddressItem"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/myInfoEditFragment_changeBirthItem" />

    <Button
        android:id="@+id/myInfoEditFragment_changeBirthButton"
        android:layout_width="wrap_content"
        android:minWidth="0dp"
        android:layout_height="40dp"
        android:layout_marginRight="16dp"
        android:backgroundTint="@color/white"
        android:text="변경"
        android:textColor="@color/black"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/myInfoEditFragment_changeBirthItem"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/myInfoEditFragment_changeBirthItem" />

    <TextView
        android:id="@+id/myInfoEditFragment_logOutTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="로그아웃"
        android:layout_marginRight="8dp"
        app:layout_constraintBottom_toBottomOf="@id/myInfoEditFragment_signOutTextView"
        app:layout_constraintEnd_toStartOf="@id/myInfoEditFragment_split"
        app:layout_constraintTop_toTopOf="@id/myInfoEditFragment_signOutTextView" />

    <View
        android:id="@+id/myInfoEditFragment_split"
        android:layout_width="0.5dp"
        android:layout_height="10dp"
        android:layout_marginRight="8dp"
        android:background="@color/kati_dark_gray"
        app:layout_constraintBottom_toBottomOf="@+id/myInfoEditFragment_signOutTextView"
        app:layout_constraintEnd_toStartOf="@+id/myInfoEditFragment_signOutTextView"
        app:layout_constraintTop_toTopOf="@+id/myInfoEditFragment_signOutTextView" />

    <TextView
        android:id="@+id/myInfoEditFragment_signOutTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="16dp"
        android:layout_marginVertical="8dp"
        android:text="회원탈퇴"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/myInfoEditFragment_editPasswordItem" />


</androidx.constraintlayout.widget.ConstraintLayout>